{
  "$schema": "https://json.openmrs.org/routes.schema.json",
  "backendDependencies": {
    "webservices.rest": "^2.2.0"
  },
  "pages": [
    {
      "component": "root",
      "route": "search"
    }
  ],
  "extensions": [
    {
      "name": "patient-search-icon",
      "component": "patientSearchIcon",
      "slot": "top-nav-actions-slot",
