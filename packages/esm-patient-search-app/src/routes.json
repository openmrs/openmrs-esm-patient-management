    {
      "name": "patient-search-button",
      "component": "patientSearchButton",
      "slot": "patient-search-button-slot",
      "offline": true
    },
    {
      "name": "patient-search-bar",
      "component": "patientSearchBar",
      "slot": "patient-search-bar-slot",
      "offline": true
    }
  ]
